{
  "name": "create-1kit-app",
  "version": "0.6.1",
  "description": "1kit is a toolkit for building web applications.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "goyalsamarth",
    "url": "https://goyalsamarth.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/igoyalsamarth/1kit",
    "directory": "packages/create-1kit-app"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "1kit",
    "toolkit",
    "web",
    "application"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "release": "changeset version",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "test": "vitest run"
  },
  "dependencies": {
    "@antfu/ni": "^23.2.0",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.0",
    "kleur": "^4.1.5",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "ts-morph": "^18.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.3"
  }
}
